   ],
   "source": [
    "# Make Normalisierung with L1\n",
    "X1_l1 = X1 / l1(X1)\n",
    "X2_l1 = X2 / l1(X2)\n",
    "plt.scatter(X1_l1, X2_l1)\n",
    "plt.xlabel(\"x1 - l1\")\n",
    "plt.ylabel(\"x2 - l1\")\n",
    "plt.show()"
