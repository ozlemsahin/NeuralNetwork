   "metadata": {},
   "outputs": [],
   "source": [
    "#Standartizierung\n",
    "def standartizierung(X, M, ss):\n",
    "    X_stndrt = np.ones(200)\n",
    "    X_stndrt = X_stndrt.reshape(1,200)\n",
    "    \n",
    "    for i in range (0, 200):\n",
    "        X_stndrt[0,i] = (X[0,i] - M) / ss\n",
    "    return X_stndrt\n",
    "\n",
    "def mean(X):\n",
    "    summe = 0.0\n",
    "    for i in range (0, 200):\n",
    "        summe = X[0,i] + summe\n",
    "    return summe / 200\n",
    "\n",
    "def ss(X):\n",
    "    return statistics.stdev(X)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 61,
   "metadata": {},
   "outputs": [
